/// Civilian

<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<ThingDef ParentName="ApparelBase">
		<defName>DMS_Apparel_UnitSuit</defName>
		<label>Uniform</label>
		<description>A jumpsuit for colonization fleet crew members and standard basic infantrymen.</description>
		<techLevel>Industrial</techLevel>
		<recipeMaker>
			<workSpeedStat>GeneralLaborSpeed</workSpeedStat>
			<workSkill>Crafting</workSkill>
			<effectWorking>Tailor</effectWorking>
			<soundWorking>Recipe_Tailor</soundWorking>
			<recipeUsers>
				<li>ElectricTailoringBench</li>
				<li>HandTailoringBench</li>
			</recipeUsers>	
			<researchPrerequisite>DMS_EquipmentBasis</researchPrerequisite>
			<unfinishedThingDef>UnfinishedApparel</unfinishedThingDef>
		</recipeMaker>
		<costStuffCount>75</costStuffCount>
		<stuffCategories>
			<li>Fabric</li>
			<li>Leathery</li>
		</stuffCategories>
		<tradeTags>
			<li>Clothing</li>
			<li>DMS_Clothing</li>
		</tradeTags>
		<graphicData>
			<texPath>Things/Apparel/UnitSuit/Apparel</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<thingCategories>
			<li>ApparelArmor</li>
		</thingCategories>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<WorkToMake>5000</WorkToMake>
			<Mass>0.5</Mass>
			<StuffEffectMultiplierArmor>0.4</StuffEffectMultiplierArmor>
			<StuffEffectMultiplierInsulation_Cold>0.7</StuffEffectMultiplierInsulation_Cold>
			<StuffEffectMultiplierInsulation_Heat>0.7</StuffEffectMultiplierInsulation_Heat>
			<EquipDelay>3</EquipDelay>
		</statBases>
		<equippedStatOffsets>
			<MoveSpeed>0.15</MoveSpeed>
			<PainShockThreshold>0.05</PainShockThreshold>
		</equippedStatOffsets>
		<apparel>
			<wornGraphicPath>Things/Apparel/UnitSuit/Apparel</wornGraphicPath>
			<blocksAddedPartWoundGraphics>true</blocksAddedPartWoundGraphics>
			<bodyPartGroups>
				<li>Torso</li>
				<li>Neck</li>
				<li>Shoulders</li>
				<li>Arms</li>
				<li>Legs</li>
			</bodyPartGroups>
			<layers>
				<li>OnSkin</li>
			</layers>
			<tags>
				<li>DMS_Uniform</li>
				<li>DMS_Officer</li>
			</tags>
			<defaultOutfitTags>
				<li>Worker</li>
				<li>Soldier</li>
			</defaultOutfitTags>
			<canBeGeneratedToSatisfyWarmth>true</canBeGeneratedToSatisfyWarmth>
			<canBeGeneratedToSatisfyToxicEnvironmentResistance>false</canBeGeneratedToSatisfyToxicEnvironmentResistance>
		</apparel>
	</ThingDef>

	<ThingDef ParentName="DMS_CapBase"><!-- GarrisionCap -->
		<defName>DMS_Apparel_GarrisionCap</defName>
		<label>Garrision Cap</label>
		<description>A soft cap for servicemen for the times when a helmet is not required or available.</description>
		<graphicData>
			<texPath>Things/Apparel/GarrisionCap/headgear_icon</texPath>
		</graphicData>
		<recipeMaker>
			<researchPrerequisite>DMS_EquipmentBasis</researchPrerequisite>
		</recipeMaker>
		<costStuffCount>45</costStuffCount>
		<stuffCategories>
			<li>Fabric</li>
			<li>Leathery</li>
		</stuffCategories>
		<statBases>
			<MaxHitPoints>50</MaxHitPoints>
			<WorkToMake>2500</WorkToMake>
			<Mass>0.05</Mass>
			<EquipDelay>1</EquipDelay>
		</statBases>
		<equippedStatOffsets>
			<MentalBreakThreshold>-0.10</MentalBreakThreshold>
		</equippedStatOffsets>
		<apparel>
			<wornGraphicPath>Things/Apparel/GarrisionCap/headgear</wornGraphicPath>
			<tags>
				<li>DMS_Garrision</li>
				<li>GarrisionCap</li>
			</tags>
		</apparel>
	</ThingDef>

	<ThingDef ParentName="ApparelMakeableBase">
		<defName>DMS_Apparel_GarrisionCoat</defName>
		<label>Garrision Coat</label>
		<description>A typical coat for an enlisted man, designed to keep one warm in chilly weather.</description>
		<possessionCount>1</possessionCount>
		<recipeMaker>
		  <researchPrerequisite>DMS_EquipmentBasis</researchPrerequisite>
		</recipeMaker>
		<graphicData>
			<texPath>Things/Apparel/GarrisionCoat/Apparel</texPath>
		  <graphicClass>Graphic_Single</graphicClass>
		  <drawSize>0.85</drawSize>
		</graphicData>
		<costStuffCount>80</costStuffCount>
		<stuffCategories>
		  <li>Fabric</li>
		  <li>Leathery</li>
		</stuffCategories>
		<thingCategories>
		  <li>ApparelMisc</li>
		</thingCategories>
		<statBases>
		  <MaxHitPoints>160</MaxHitPoints>
		  <WorkToMake>7000</WorkToMake>
		  <Mass>1.5</Mass>
		  <StuffEffectMultiplierArmor>0.4</StuffEffectMultiplierArmor>
		  <StuffEffectMultiplierInsulation_Cold>0.8</StuffEffectMultiplierInsulation_Cold>
		  <StuffEffectMultiplierInsulation_Heat>0.5</StuffEffectMultiplierInsulation_Heat>
		  <EquipDelay>1.5</EquipDelay>
		</statBases>
		<apparel>
		  <bodyPartGroups>
			<li>Torso</li>
			<li>Neck</li>
			<li>Shoulders</li>
			<li>Arms</li>
		  </bodyPartGroups>
		  <wornGraphicPath>Things/Apparel/GarrisionCoat/Apparel</wornGraphicPath>
		  <blocksAddedPartWoundGraphics>true</blocksAddedPartWoundGraphics>
		  <layers>
			<li>Shell</li>
		  </layers>
		  <tags>
			<li>IndustrialAdvanced</li>
			<li>DMS_Garrision</li>
			<li>DMS_Officer</li>
		  </tags>
		  <defaultOutfitTags>
			<li>Soldier</li>
		  </defaultOutfitTags>
		  <canBeDesiredForIdeo>true</canBeDesiredForIdeo>
		</apparel>
		<colorGenerator Class="ColorGenerator_Options">
		  <options>
			<li>
			  <weight>10</weight>
			  <only>(0.33,0.33,0.33)</only>
			</li>
			<li>
			  <weight>10</weight>
			  <only>(0.65,0.65,0.65)</only>
			</li>
			<li>
			  <weight>10</weight>
			  <only>(0.4,0.3,0.15)</only>
			</li>
			<li>
			  <weight>15</weight>
			  <min>(0.3,0.3,0.3)</min>
			  <max>(0.5,0.5,0.5)</max>
			</li>
			<li>
			  <weight>6</weight>
			  <min>(0.5,0.5,0.5)</min>
			  <max>(1,1,1)</max>
			</li>
		  </options>
		</colorGenerator>
		<tradeTags>
		  <li>BasicClothing</li>
		  <li>DMS_Clothing</li>
		</tradeTags>
	  </ThingDef>

	<ThingDef ParentName="ApparelBase">
		<defName>DMS_Apparel_HazardSuit</defName>
		<label>Hazard Suit</label>
		<description>A hazardous material suit covering the wearer's entire body. Used for protection against hazardous biological and chemical agents.</description>
		<techLevel>Industrial</techLevel>
		<recipeMaker>
			<workSpeedStat>GeneralLaborSpeed</workSpeedStat>
			<workSkill>Crafting</workSkill>
			<effectWorking>Tailor</effectWorking>
			<soundWorking>Recipe_Tailor</soundWorking>
			<recipeUsers>
				<li>ElectricTailoringBench</li>
				<li>HandTailoringBench</li>
			</recipeUsers>
			<researchPrerequisite>DMS_Plasfiber</researchPrerequisite>
			<unfinishedThingDef>UnfinishedTechArmor</unfinishedThingDef>
		</recipeMaker>
		<costList>
			<Plasfiber>80</Plasfiber>
			<Steel>20</Steel>
			<Chemfuel>20</Chemfuel>
			<ComponentStamped>1</ComponentStamped>
		</costList>
		<tradeTags>
			<li>Clothing</li>
			<li>DMS_Clothing</li>
		</tradeTags>
		<graphicData>
			<texPath>Things/Apparel/HazardSuit/Icon</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<thingCategories>
			<li>ApparelMisc</li>
		</thingCategories>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<WorkToMake>5000</WorkToMake>
			<Mass>10</Mass>
			<ArmorRating_Sharp>0.1</ArmorRating_Sharp>
			<ArmorRating_Blunt>0.4</ArmorRating_Blunt>
			<ArmorRating_Heat>0.8</ArmorRating_Heat>
			<Insulation_Cold>20</Insulation_Cold>
			<Insulation_Heat>9</Insulation_Heat>
			<EquipDelay>1.5</EquipDelay>
		</statBases>
		<equippedStatOffsets>
			<MoveSpeed>-0.5</MoveSpeed>
			<ToxicEnvironmentResistance>1</ToxicEnvironmentResistance>
		</equippedStatOffsets>
		<apparel>
			<bodyPartGroups>
				<li>FullHead</li>
				<li>UpperHead</li>
				<li>Neck</li>
				<li>Shoulders</li>
				<li>Torso</li>
				<li>Legs</li>
				<li>Arms</li>
				<li>Hands</li>
				<li>Feet</li>
			</bodyPartGroups>
			<wornGraphicPath>Things/Apparel/HazardSuit/Apparel</wornGraphicPath>
			<blocksAddedPartWoundGraphics>true</blocksAddedPartWoundGraphics>
			<parentTagDef>ApparelBody</parentTagDef>
			<renderSkipFlags>
			  <li>Hair</li>
			  <li>Beard</li>
			  <li>Eyes</li>
			</renderSkipFlags>
			<drawData>
			  <defaultData>
				<layer>75</layer>
			  </defaultData>
			</drawData>
			<immuneToToxGasExposure>true</immuneToToxGasExposure>
			<layers>
				<li>Shell</li>
			</layers>
			<defaultOutfitTags>
				<li>Worker</li>
			</defaultOutfitTags>
			<canBeGeneratedToSatisfyWarmth>false</canBeGeneratedToSatisfyWarmth>
			<canBeGeneratedToSatisfyToxicEnvironmentResistance>true</canBeGeneratedToSatisfyToxicEnvironmentResistance>
		</apparel>
		<modExtensions>
			<li Class="DMS.BiochemicalProtectionExtension"></li>
		</modExtensions>
	</ThingDef>

	<ThingDef ParentName="ApparelBase">
		<defName>DMS_Apparel_PowerSkelton</defName>
		<label>Powered Exoskeletal Unit</label>
		<description>A powered exoskeletal suit which mimics the human form and can be worn by an untrained operator. It significantly increases the wearer's strength and running speed.</description>
		<techLevel>Industrial</techLevel>
		<recipeMaker>
			<workSpeedStat>GeneralLaborSpeed</workSpeedStat>
			<workSkill>Crafting</workSkill>
			<effectWorking>ConstructMetal</effectWorking>
			<soundWorking>Recipe_Machining</soundWorking>
			<researchPrerequisite>DMS_MechBasis</researchPrerequisite>
			<recipeUsers>
			  <li>DMS_TableMachinePrinter</li>
			</recipeUsers>
			<unfinishedThingDef>UnfinishedTechArmor</unfinishedThingDef>
		</recipeMaker>
		<costList>
			<Tungsteel>75</Tungsteel>
			<Plasfiber>50</Plasfiber>
			<Uranium>10</Uranium>
			<ComponentStamped>7</ComponentStamped>
		</costList>
		<tradeTags>
			<li>DMS_Armor</li>
		</tradeTags>
		<graphicData>
			<texPath>Things/Apparel/ExoSkelton/Apparel</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<thingCategories>
			<li>ApparelArmor</li>
		</thingCategories>
		<statBases>
			<MaxHitPoints>120</MaxHitPoints>
			<WorkToMake>8000</WorkToMake>
			<Mass>9</Mass>
			<ArmorRating_Sharp>0.35</ArmorRating_Sharp>
			<ArmorRating_Blunt>0.35</ArmorRating_Blunt>
			<ArmorRating_Heat>0.6</ArmorRating_Heat>
			<Insulation_Cold>0</Insulation_Cold>
			<Insulation_Heat>0</Insulation_Heat>
			<EquipDelay>30</EquipDelay>
		</statBases>
		<equippedStatOffsets>
			<MoveSpeed>-0.5</MoveSpeed>
			<VEF_MassCarryCapacity>75</VEF_MassCarryCapacity>
		</equippedStatOffsets>
		<apparel>
			<bodyPartGroups>
				<li>Hands</li>
				<li>Feet</li>
				<li>Legs</li>
				<li>Waist</li>
			</bodyPartGroups>
			<drawData>
				<defaultData>
					<layer>30</layer>
				</defaultData>
				<dataNorth>
					<layer>88</layer>
				</dataNorth>
			</drawData>
			<wornGraphicPath>Things/Apparel/ExoSkelton/Apparel</wornGraphicPath>
			<blocksAddedPartWoundGraphics>true</blocksAddedPartWoundGraphics>
			<layers>
				<li>Shell</li>
			</layers>
			<defaultOutfitTags>
				<li>Soldier</li>
			</defaultOutfitTags>
			<canBeGeneratedToSatisfyWarmth>false</canBeGeneratedToSatisfyWarmth>
			<canBeGeneratedToSatisfyToxicEnvironmentResistance>false</canBeGeneratedToSatisfyToxicEnvironmentResistance>
		</apparel>
		<modExtensions>
			<li Class="VFECore.ApparelExtension">
				<pawnCapacityMinLevels>
					<li>
						<capacity>Moving</capacity>
						<minLevel>0.75</minLevel>
					</li>
					<li>
						<capacity>Manipulation</capacity>
						<minLevel>0.75</minLevel>
					</li>
				</pawnCapacityMinLevels>
			</li>
		</modExtensions>
	</ThingDef>

</Defs>


/// Military

<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<!-- 仿生作戰服 -->
	<ThingDef ParentName="ApparelBase">
		<defName>DMS_Apparel_BionicSuit</defName>
		<label>Bionic Suit</label>
		<description>A step above the civilian model Powered Exoskeleton Suit, this military grade bionic suit is sewn from artificial muscle fibers and amplifies the wearers pre-existing muscular strength. Greatly increases the wearers carrying capacity and movement speed.</description>
		<techLevel>Industrial</techLevel>
		<recipeMaker>
			<workSpeedStat>GeneralLaborSpeed</workSpeedStat>
			<workSkill>Crafting</workSkill>
			<effectWorking>Smith</effectWorking>
			<soundWorking>Recipe_Machining</soundWorking>
			<recipeUsers>
				<li>DMS_TableArtifuscle</li>
			</recipeUsers>
			<researchPrerequisite>DMS_Artifuscle</researchPrerequisite>
			<unfinishedThingDef>UnfinishedTechArmor</unfinishedThingDef>
		</recipeMaker>
		<costList>
			<Plasfiber>40</Plasfiber>
			<Artifuscle>75</Artifuscle>
			<Chemfuel>20</Chemfuel>
		</costList>
		<tradeTags>
			<li>DMS_Armor</li>
		</tradeTags>
		<graphicData>
			<texPath>Things/Apparel/BionicSuit/Apparel</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<thingCategories>
			<li>ApparelArmor</li>
		</thingCategories>
		<statBases>
			<WorkToMake>8000</WorkToMake>
			<Mass>7</Mass>
			<ArmorRating_Sharp>0.4</ArmorRating_Sharp>
			<ArmorRating_Blunt>0.4</ArmorRating_Blunt>
			<ArmorRating_Heat>0.6</ArmorRating_Heat>
			<Insulation_Cold>18</Insulation_Cold>
			<Insulation_Heat>18</Insulation_Heat>
			<EquipDelay>15</EquipDelay>
			<DeteriorationRate>0</DeteriorationRate>
			<Flammability>0</Flammability>
		</statBases>
		<useHitPoints>false</useHitPoints>
		<equippedStatOffsets>
			<MoveSpeed>1.75</MoveSpeed>
			<RestFallRateFactor>0.25</RestFallRateFactor>
			<ToxicEnvironmentResistance>0.25</ToxicEnvironmentResistance>
		</equippedStatOffsets>
		<apparel>
			<bodyPartGroups>
				<li>Neck</li>
				<li>Shoulders</li>
				<li>Torso</li>
				<li>Legs</li>
				<li>Arms</li>
			</bodyPartGroups>
			<wornGraphicPath>Things/Apparel/BionicSuit/Apparel</wornGraphicPath>
			<blocksAddedPartWoundGraphics>true</blocksAddedPartWoundGraphics>
			<layers>
				<li>OnSkin</li>
			</layers>
			<defaultOutfitTags>
				<li>Soldier</li>
			</defaultOutfitTags>
			<canBeGeneratedToSatisfyWarmth>false</canBeGeneratedToSatisfyWarmth>
			<canBeGeneratedToSatisfyToxicEnvironmentResistance>false</canBeGeneratedToSatisfyToxicEnvironmentResistance>
		</apparel>
	</ThingDef>

